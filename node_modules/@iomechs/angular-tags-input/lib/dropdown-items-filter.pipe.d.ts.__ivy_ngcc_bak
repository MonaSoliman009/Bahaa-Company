import { PipeTransform } from '@angular/core';
import { AngularTagItem, AngularTagsInputConfig } from './tags-input-interfaces';
export declare class DropdownItemsFilterPipe implements PipeTransform {
    transform(tagItems: Array<AngularTagItem>, config: AngularTagsInputConfig, searchTerm?: string): any;
    getFilteredItem(item: AngularTagItem, config: AngularTagsInputConfig, searchTerm?: string): AngularTagItem;
}
