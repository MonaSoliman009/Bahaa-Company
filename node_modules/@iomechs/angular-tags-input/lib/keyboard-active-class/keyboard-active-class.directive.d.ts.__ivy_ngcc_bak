import { OnChanges, SimpleChanges, ElementRef, ChangeDetectorRef, OnInit } from '@angular/core';
export declare class KeyboardActiveClassDirective implements OnInit, OnChanges {
    private el;
    private cdRef;
    isKeyboardActiveItem: boolean;
    tiKeyboardActiveClass: string;
    constructor(el: ElementRef, cdRef: ChangeDetectorRef);
    ngOnInit(): void;
    ngOnChanges(simpleChanges: SimpleChanges): void;
    evaluateClassApplication(): void;
}
