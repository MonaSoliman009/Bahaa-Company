import { AngularTagsInputConfig } from './tags-input-interfaces';
export declare class AngularTagsInputService {
    debugMode: boolean;
    constructor();
    /**
     * @author Ahsan Ayaz
     * @desc Finds a tag by Id
     * @param tagsData Array of the tags (could be heirarchical)
     * @param tagId identifier of the tag to be found
     * @param config the tags input config
     * @returns the tag found, or `undefined`
     */
    findTagById(tagsData: any, tagId: any, config: AngularTagsInputConfig): any;
    setDebugMode(mode: boolean): void;
    log(logMessage: any, logLabel?: string): void;
    findParent(tagsArray: any, tagItem: any, config: AngularTagsInputConfig): any;
    getMainTagAfterAdding(tagsArray: any, tagAdded: any, tags: any, config: AngularTagsInputConfig): any;
}
