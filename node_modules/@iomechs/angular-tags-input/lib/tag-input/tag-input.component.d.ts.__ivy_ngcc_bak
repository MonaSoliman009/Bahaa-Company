import { OnInit, EventEmitter, ElementRef, SimpleChanges, OnChanges } from '@angular/core';
import { AngularTagItem, AngularTagsInputConfig } from '../tags-input-interfaces';
import { FormGroup } from '@angular/forms';
export declare class TagInputComponent implements OnInit, OnChanges {
    inputClass: string;
    config: AngularTagsInputConfig;
    disabled: boolean;
    valueChanged: EventEmitter<string>;
    inputFocused: EventEmitter<string>;
    inputBlurred: EventEmitter<string>;
    tagEntered: EventEmitter<AngularTagItem>;
    inputEl: ElementRef;
    tagInputForm: FormGroup;
    lastValueEmitted: string;
    constructor();
    ngOnInit(): void;
    ngOnChanges(changes: SimpleChanges): void;
    /**
     * @author Ahsan Ayaz
     * @desc Emits the input focused event with the current search term value
     */
    emitInputFocused(): void;
    /**
     * @author Ahsan Ayaz
     * @desc When the user presses enter key after entering the tag name
     * @param inputEl - Reference to the input element so we can fetch the value
     */
    onEnterKeyPress(): void;
    /**
     * @author Ahsan Ayaz
     * @desc Resets the input value
     */
    resetInput(): void;
    emitInputBlurred($event: any): void;
}
